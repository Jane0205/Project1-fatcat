<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#000000"
    tools:context="com.example.android.myapplication.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="musical offerings/mission/learn/fab lab"
        android:textColor="#FFFFFF"
        android:layout_marginTop="13dp"
        android:textSize="13dp"
        android:layout_centerHorizontal="true"
        android:fontFamily="serif-monospace"
        android:textStyle="bold"
        />

    <TextView
        android:layout_width="348dp"
        android:layout_height="wrap_content"
        android:text="\n  event/pool,pong and more/shop/contact"
        android:textColor="#FFFFFF"
        android:layout_marginTop="15dp"
        android:layout_centerHorizontal="true"
        android:textSize="13dp"
        android:fontFamily="serif-monospace"
        android:textStyle="bold"/>

<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_weight="1"
        android:src="@drawable/fatcat2"
        android:layout_marginTop="55dp"
        />
    <ImageView
    android:layout_width="wrap_content"
    android:layout_height="200dp"
    android:layout_weight="1"
    android:src="@drawable/fatcat"
     android:layout_marginTop="55dp"
     />
</LinearLayout>

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/instagram"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="70dp"
        android:id="@+id/insta"
        android:layout_weight="1"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/twitter"
        android:layout_toRightOf="@+id/insta"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/twit"
        android:layout_weight="1"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/pinterest"
        android:layout_toRightOf="@+id/twit"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/pin"
        android:layout_weight="1"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/facebook"
        android:layout_toRightOf="@+id/pin"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/face"
        android:layout_weight="1"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/youtube"
        android:layout_toRightOf="@+id/face"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/you"
        android:layout_weight="1"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/foursquare"
        android:layout_toRightOf="@+id/you"
        android:layout_marginTop="263dp"
        android:layout_marginLeft="5dp"
        android:layout_weight="1"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" It's All Jazz and Games at Fat Cat"
        android:textColor="#FF0000"
        android:textSize="16dp"
        android:layout_marginTop="300dp"
        android:fontFamily="sans-serif-smallcaps"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shufflepuck, ping pong, billiards, oh my! This downtown cellar is the place for adults to be a kid again and Maria Sansone does just that--making new friends, dominating the games, and enjoying the cool vibes of some downtown jazz. An all-nighter might be a little too easy at this West Village standby."
        android:textColor="#ffffff"
        android:textSize="11.5dp"
        android:layout_marginTop="325dp"
        android:fontFamily="sans-serif-smallcaps"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call:212.675.6056 \nAddress:75 Christopher at 7th Ave.\nOpen hour : 2pm-5am M-T / 12pm-5am F-S"
        android:layout_marginTop="405dp"
        android:textColor="#FF4500"
        android:fontFamily="serif-monospace"/>

    <ImageView
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:src="@drawable/miniButton"
        android:layout_marginTop="450dp"
        android:layout_marginLeft="280dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Click here to sign up for our mailing liks Check out our reviews:   YELP.COM  Or press - Check us out"
        android:textSize="10dp"
        android:textColor="#A9A9A9"
        android:layout_alignParentBottom="true" />
</RelativeLayout>